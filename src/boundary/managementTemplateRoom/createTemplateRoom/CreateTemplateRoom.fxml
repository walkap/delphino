            <Label layoutX="40.0" layoutY="310.0" text="Desk" AnchorPane.leftAnchor="50.0" />
            <CheckBox fx:id="desk" layoutX="216.0" layoutY="310.0" mnemonicParsing="false" AnchorPane.leftAnchor="300.0" />
