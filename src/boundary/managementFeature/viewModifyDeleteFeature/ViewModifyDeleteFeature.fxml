<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="boundary.managementFeature.viewModifyDeleteFeature.ViewModifyDeleteFeatureUI">
   <children>
      <Button fx:id="homePageBtn" layoutX="437.0" layoutY="28.0" mnemonicParsing="false" text="Hompe Page" AnchorPane.rightAnchor="80.0" />
      <Button fx:id="exitBtn" layoutX="512.0" layoutY="28.0" mnemonicParsing="false" onAction="#closeWindow" text="Exit" AnchorPane.rightAnchor="20.0" />
      <Button fx:id="prevPageBtn" layoutX="512.0" layoutY="78.0" mnemonicParsing="false"  text="Previous Page" AnchorPane.rightAnchor="20.0" />
      <Label layoutX="81.0" layoutY="30.0" text="View and Delete Feature">
         <font>
            <Font name="System Bold Italic" size="18.0" />
         </font>
      </Label>
      <Button fx:id="deleteBtn" layoutX="443.0" layoutY="320.0" mnemonicParsing="false" onAction="#deleteFeature" text="Delete" />
      <TextField fx:id="nameFeature" editable="false" layoutX="164.0" layoutY="136.0" />
      <TextField fx:id="descriptionFeature" layoutX="166.0" layoutY="212.0" prefHeight="31.0" prefWidth="371.0" />
      <ListView fx:id="listView" onMouseClicked="#chooseFeatureAndFillTV" layoutX="14.0" layoutY="136.0" prefHeight="200.0" prefWidth="106.0" />
      <Label layoutX="20.0" layoutY="109.0" text="Name Feature" />
      <Label layoutX="164.0" layoutY="109.0" text="Name" />
      <Label layoutX="167.0" layoutY="179.0" text="Description" />
   </children>
</AnchorPane>
