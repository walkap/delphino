    <children>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
            <children>
                <Button layoutX="235.0" layoutY="200.0" mnemonicParsing="false" onAction="#createBuilding"
                        text="Crea edificio"/>
                <TextField fx:id="name" layoutX="149.0" layoutY="108.0"/>
                <TextField fx:id="area" layoutX="149.0" layoutY="152.0" prefHeight="26.0" prefWidth="53.0"/>
                <Label layoutX="27.0" layoutY="108.0" prefHeight="26.0" prefWidth="45.0" text="Nome"/>
                <Label layoutX="20.0" layoutY="157.0" text="Macroarea"/>
                <Label layoutX="244.0" layoutY="36.0" prefHeight="51.0" prefWidth="152.0" text="Creazione Edificio"/>
            </children>
        </AnchorPane>
    </children>
</VBox>
